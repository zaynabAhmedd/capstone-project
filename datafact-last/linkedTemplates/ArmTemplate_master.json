{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafact-last"
		},
		"ls_db_ml_post_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_db_ml_post'"
		},
		"ls_rds_pg_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_rds_pg'"
		},
		"ls_wcd_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_wcd_blob'"
		},
		"ls_db_ml_post_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0511-085359-y3wdvsrq"
		},
		"ls_my_blob_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://sdacapstoneproject.blob.core.windows.net/"
		},
		"ls_rds_pg_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "de-rds.czm23kqmbd6o.ca-central-1.rds.amazonaws.com"
		},
		"ls_rds_pg_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "stack"
		},
		"ls_rds_pg_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "postgres"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafact-last_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_db_ml_post_accessToken": {
						"value": "[parameters('ls_db_ml_post_accessToken')]"
					},
					"ls_rds_pg_password": {
						"value": "[parameters('ls_rds_pg_password')]"
					},
					"ls_wcd_blob_connectionString": {
						"value": "[parameters('ls_wcd_blob_connectionString')]"
					},
					"ls_db_ml_post_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_db_ml_post_properties_typeProperties_existingClusterId')]"
					},
					"ls_my_blob_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('ls_my_blob_properties_typeProperties_serviceEndpoint')]"
					},
					"ls_rds_pg_properties_typeProperties_server": {
						"value": "[parameters('ls_rds_pg_properties_typeProperties_server')]"
					},
					"ls_rds_pg_properties_typeProperties_database": {
						"value": "[parameters('ls_rds_pg_properties_typeProperties_database')]"
					},
					"ls_rds_pg_properties_typeProperties_username": {
						"value": "[parameters('ls_rds_pg_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}